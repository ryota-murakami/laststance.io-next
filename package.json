  "author": "Ryota Murakami <dojce1048@gmail.com> (https://github.com/ryota-murakami)",
  "packageManager": "pnpm@8.10.2",
  "private": true,
  "volta": {
    "node": "18.18.2"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "browserslist": [
    "last 1 version"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,jsx,cjs,mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,jsx,cjs,mjs --fix",
    "prettier": "prettier --ignore-unknown --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .next node_modules tsconfig.tsbuildinfo pnpm-lock.yaml",
    "generate-blog-templete": "node ./scripts/generate-blog-templete.mjs",
    "validate": "./scripts/validate",
    "playwright:test": "exec playwright test",
    "playwright:ui": "exec playwright test --ui",
    "playwright:headed": "exec playwright test --headed",
    "playwright:debug": "exec playwright test --debug",
    "playwright:desktop:codegen": "exec playwright codegen --device 'chromium'",
    "playwright:tablet:codegen": "exec playwright codegen --device 'iPad Pro 11'",
    "playwright:mobile:codegen": "exec playwright codegen --device 'Pixel 5'"
  },
